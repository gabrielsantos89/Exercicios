void main() {
/*3. Faça uma função que recebe por parâmetro um valor inteiro e positivo e retorna o valor lógico Verdadeiro caso o valor seja primo e Falso em caso contrário.*/
  
  
 if(primoNprimo(5))
   print("É Primo");
  else
   print("Não é Primo");
}

  
bool primoNprimo(int n){
  int cont = 0;
  for(int i=0;i<=n;i++){
    if(n % i == 0)
      cont++;
  }
  if(cont==2)
    return true;
  else
    return false;
  }
