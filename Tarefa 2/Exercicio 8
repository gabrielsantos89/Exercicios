void main() {
/*8. Faça uma função que recebe a idade de um nadador por parâmetro e retorna , também por parâmetro, a categoria desse nadador de acordo com a tabela abaixo (imagem2):*/
  
  print(categoria(8));
  
}

  
String categoria(int idade){
  if(idade >4 && idade<8){
    return "Infantil A";}
  if(idade>=8 && idade<=10){
    return "Infantil B";}
  if(idade>10 && idade<=13){
     return "Juvenil A";}
  if(idade>13 && idade<=17){
     return "Juvenil B";}
  if(idade>17){
    return "Adulto";}
  else{
    return "Dados invalidos";}
  }
